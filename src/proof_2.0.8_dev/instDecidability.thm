Define nVar : olist -> nat -> prop by
	nVar nil z;
	nabla x, nVar (tyvar x :: E) N := nVar E N;
	nabla x, nVar (styvar x :: E) N := nVar E N;
	nabla x, nVar (exvar x :: E) (s N) := nVar E N;
	nabla x, nVar (of x A :: E) N := nVar E N;
	nVar (j J :: E) N := nVar E N.

