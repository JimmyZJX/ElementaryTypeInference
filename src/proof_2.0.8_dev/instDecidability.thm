Import "completeness".

Theorem is_nat_prune[A] : forall N, nabla (x : A),
	is_nat (N x) -> exists Nr, N = x\ Nr.
induction on 1. intros. case H1. search.
	apply IH to H2. /* GEN_BY_SCRIPT 
	applys IH. */  search.

Define nTopBot : ty -> nat -> prop by
	nTopBot i z;
	nTopBot top (s z);
	nTopBot bot (s z);
	nabla x, nTopBot x z;
	nTopBot (arrow A B) K := exists M N, nTopBot A M /\ nTopBot B N /\ add M N K;
	nTopBot (all A) N := nabla x, nTopBot (A x) N.

Theorem nTopBot_is_nat : forall J N, nTopBot J N -> is_nat N.
induction on 1. intros. case H1.
	search. search. search. search.
	apply IH to H2. /* GEN_BY_SCRIPT 
	applys IH H2. */  apply IH to H3. /* GEN_BY_SCRIPT  applys IH H3. */  apply add_is_nat_k to _ _. /* GEN_BY_SCRIPT  applys add_is_nat_k. */  search.
	apply IH to H2. /* GEN_BY_SCRIPT 
	applys IH. */  search.
Theorem nTopBot_prune : forall J N, nabla (x : ty),
	nTopBot (J x) (N x) -> exists Nr, N = x\ Nr.
intros. apply nTopBot_is_nat to H1. /* GEN_BY_SCRIPT  applys nTopBot_is_nat H1. */  apply is_nat_prune to H2. /* GEN_BY_SCRIPT  applys is_nat_prune H2. */  search.
Theorem nTopBot_wf_total : forall E A, wfta E A -> exists NTB, nTopBot A NTB.
induction on 1. intros. case H1. % Manual Fix try search.
 search. search. search. search. search. search.
	apply IH to H2. /* GEN_BY_SCRIPT 
	applys IH H2. */  apply IH to H3. /* GEN_BY_SCRIPT  applys IH H3. */ 
		apply nTopBot_is_nat to H4. /* GEN_BY_SCRIPT 
		applys nTopBot_is_nat H4. */ 
		apply add_total to H6 with n = NTB1. /* GEN_BY_SCRIPT 
		applys add_total H6 with n = NTB1. */ 
		search.
	apply IH to H2. /* GEN_BY_SCRIPT 
	applys IH. */  apply nTopBot_is_nat to H4. /* GEN_BY_SCRIPT  applys nTopBot_is_nat H4. */  apply is_nat_prune to H5. /* GEN_BY_SCRIPT  applys is_nat_prune H5. */  search.

Define nTBJ : judgment -> nat -> prop by
	nTBJ (subty A B) K := exists M N, nTopBot A M /\ nTopBot B N /\ add M N K;
	nTBJ (check X A) z;
	nTBJ (inferC X J) z;
	nTBJ (typeAppC A B J) z;
	nTBJ (inferAppC A X J) z.

Theorem nTBJ_is_nat : forall J N, nTBJ J N -> is_nat N.
induction on 1. intros. case H1. % Manual Fix try search.
	apply nTopBot_is_nat to H3. /* GEN_BY_SCRIPT 
	applys nTopBot_is_nat H3. */  apply add_is_nat_k to _ _. /* GEN_BY_SCRIPT  applys add_is_nat_k. */  search.
	 search. search. search. search.
Theorem nTBJ_prune : forall J N, nabla (x : ty),
	nTBJ (J x) (N x) -> exists Nr, N = x\ Nr.
intros. apply nTBJ_is_nat to H1. /* GEN_BY_SCRIPT  applys nTBJ_is_nat H1. */  apply is_nat_prune to H2. /* GEN_BY_SCRIPT  applys is_nat_prune H2. */  search.
Theorem nTBJ_prune_tm : forall J N, nabla (x : tm),
	nTBJ (J x) (N x) -> exists Nr, N = x\ Nr.
intros. apply nTBJ_is_nat to H1. /* GEN_BY_SCRIPT  applys nTBJ_is_nat H1. */  apply is_nat_prune to H2. /* GEN_BY_SCRIPT  applys is_nat_prune H2. */  search.

Define nTB : olist -> nat -> prop by
	nTB nil z;
	nabla x, nTB (tyvar x :: Exp x) N := nabla x, nTB (Exp x) N;
	nabla x, nTB (styvar x :: Exp x) N := nabla x, nTB (Exp x) N;
	nabla x, nTB (exvar x :: Exp x) N := nabla x, nTB (Exp x) N;
	nabla x, nTB (of x A :: Exp x) N := nabla x, nTB (Exp x) N;
	nTB (j J :: Exp) K := exists M N, nTBJ J M /\ nTB Exp N /\ add M N K.

Theorem nTB_is_nat : forall J N, nTB J N -> is_nat N.
induction on 1. intros. case H1. search. % Manual Fix
	apply IH to H2. search.
	apply IH to H2. search.
	apply IH to H2. search.
	apply IH to H2. search.
	apply IH to H3. apply add_is_nat_k to _ _. /* GEN_BY_SCRIPT 
	applys add_is_nat_k. */  search.

Theorem nTB_prune : forall J N, nabla (x : ty),
	nTB (J x) (N x) -> exists Nr, N = x\ Nr.
intros. apply nTB_is_nat to H1. /* GEN_BY_SCRIPT  applys nTB_is_nat H1. */  apply is_nat_prune to H2. /* GEN_BY_SCRIPT  applys is_nat_prune H2. */  search.
Theorem nTB_prune_tm : forall J N, nabla (x : tm),
	nTB (J x) (N x) -> exists Nr, N = x\ Nr.
intros. apply nTB_is_nat to H1. /* GEN_BY_SCRIPT  applys nTB_is_nat H1. */  apply is_nat_prune to H2. /* GEN_BY_SCRIPT  applys is_nat_prune H2. */  search.

